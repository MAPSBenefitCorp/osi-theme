{
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "var(--wp--custom--content-size)",
      "wideSize": "var(--wp--custom--wide-size)"
    },
    "color": {
      "custom": true,
      "customGradient": true,
      "link": true,
      "duotone": [],
      "gradients": [],
      "defaultPalette": false,
      "palette": [
        {
          "slug": "black",
          "color": "#1D1D1D",
          "name": "Black"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        },
        {
          "slug": "transparent",
          "color": "transparent",
          "name": "Transparent"
        },
        {
          "slug": "primary-300",
          "color": "#3da639",
          "name": "Primary 300"
        },
        {
          "slug": "primary-500",
          "color": "#3da639",
          "name": "Primary 500"
        },
        {
          "slug": "primary-600",
          "color": "#3da639",
          "name": "Primary 600"
        },
        {
          "slug": "primary-700",
          "color": "#3da639",
          "name": "Primary 700"
        },
        {
          "slug": "neutral-100",
          "color": "#F7F7F7",
          "name": "Neutral 100"
        },
        {
          "slug": "neutral-200",
          "color": "#EFEFEF",
          "name": "Neutral 200"
        },
        {
          "slug": "neutral-300",
          "color": "#CECECE",
          "name": "Neutral 300"
        },
        {
          "slug": "neutral-500",
          "color": "#767676",
          "name": "Neutral 500"
        },
        {
          "slug": "neutral-700",
          "color": "#474747",
          "name": "Neutral 700"
        },
        {
          "slug": "neutral-900",
          "color": "#232323",
          "name": "Neutral 900"
        }
      ]
    },
    "custom": {
      "contentSize": "min( calc(100vw - 30px), 620px)",
      "wideSize": "min( calc(100vw - 30px), 1240px)",
      "lineHeight": {
        "small": 1.2,
        "headings": 1.3,
        "body": 1.6
      },
      "fontWeight": {
        "normal": 400,
        "bold": 500,
        "bolder": 700
      },
      "letterSpacing": "0.2px",
      "spacing": {
        "small": "10px",
        "medium": "20px",
        "large": "30px",
        "row": "60px",
        "section": "90px"
      },
      "color": {
        "primary-button-hover": "var(--wp--preset--color--primary-600)",
        "secondary-button-hover": "#474747",
        "border": "var(--wp--preset--color--primary-500)",
        "link": "var(--wp--preset--color--primary-500)",
        "link-hover": "var(--wp--preset--color--primary-600)",
        "hr": "var(--wp--preset--color--neutral-500)"
      },
      "mobile": {
        "fontSize": {
          "xl": "1.1875rem",
          "2-xl": "1.25rem",
          "3-xl": "1.375rem",
          "4-xl": "1.5rem",
          "5-xl": "1.75rem;",
          "6-xl": "2rem"
        },
        "spacing": {
          "small": "8px",
          "medium": "12px",
          "large": "18px",
          "row": "20px",
          "section": "25px"
        }
      }
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fontFamilies": [
        {
          "fontFamily": "Poppins, sans-serif",
          "slug": "primary",
          "name": "Primary"
        },
        {
          "fontFamily": "Libre Baskerville, serif",
          "slug": "headings",
          "name": "Headings"
        }
      ],
      "fontSizes": [
        {
          "slug": "xs",
          "size": "0.875rem",
          "name": "XS"
        },
        {
          "slug": "sm",
          "size": "0.9375rem",
          "name": "SM"
        },
        {
          "slug": "base",
          "size": "1rem",
          "name": "Base"
        },
        {
          "slug": "lg",
          "size": "1.125rem",
          "name": "LG"
        },
        {
          "slug": "xl",
          "size": "1.25rem",
          "name": "XL"
        },
        {
          "slug": "2-xl",
          "size": "1.5rem",
          "name": "2XL"
        },
        {
          "slug": "3-xl",
          "size": "1.875rem",
          "name": "3XL"
        },
        {
          "slug": "4-xl",
          "size": "2.125rem",
          "name": "4XL"
        },
        {
          "slug": "5-xl",
          "size": "2.375rem",
          "name": "5XL"
        },
        {
          "slug": "6-xl",
          "size": "2.625rem",
          "name": "6XL"
        }
      ]
    },
    "spacing": {
      "blockGap": true,
      "units": [
        "px",
        "em",
        "rem",
        "%",
        "vw",
        "vh"
      ]
    },
    "blocks": {
      "core/paragraph": {
        "border": {},
        "color": {},
        "custom": {},
        "layout": {}
      }
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--black)",
      "background": "var(--wp--preset--color--white)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--lg)",
      "lineHeight": "var(--wp--custom--line-height--body)"
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary-500)"
        },
        "typography": {
          "textDecoration": "underline"
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--headings)",
          "fontSize": "var(--wp--preset--font-size--6-xl)",
          "lineHeight": "var(--wp--custom--line-height--headings)",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        },
        "spacing": {
          "margin": {
            "top": ""
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--headings)",
          "fontSize": "var(--wp--preset--font-size--4-xl)",
          "lineHeight": "var(--wp--custom--line-height--headings)",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        },
        "spacing": {
          "margin": {
            "top": ""
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--headings)",
          "fontSize": "var(--wp--preset--font-size--3-xl)",
          "lineHeight": "var(--wp--custom--line-height--headings)",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--headings)",
          "fontSize": "var(--wp--preset--font-size--2-xl)",
          "lineHeight": "var(--wp--custom--line-height--headings)",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--xl)",
          "lineHeight": "var(--wp--custom--line-height--headings)",
          "fontWeight": "var(--wp--custom--font-weight--bolder)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--lg)",
          "lineHeight": "var(--wp--custom--line-height--primary)",
          "fontWeight": "var(--wp--custom--font-weight--bolder)"
        }
      }
    },
    "blocks": {
      "core/spacer": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/post-title": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--black)"
            },
            "typography": {
              "fontFamily": "var(--wp--preset--font-family--headings)",
              "fontWeight": "var(--wp--custom--font-weight--normal)",
              "lineHeight": "var(--wp--custom--line-height--headings)",
              "textDecoration": "none"
            }
          }
        }
      },
      "core/post-featured-image": {
        "spacing": {
          "margin": {
            "top": "var(--wp--custom--spacing--large)",
            "bottom": "var(--wp--custom--spacing--large)"
          }
        }
      },
      "core/post-content": {
      },
      "core/post-comments": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--section) !important"
          }
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0.7em"
          }
        }
      },
      "core/button": {
        "border": {
          "radius": "0px"
        },
        "color": {
          "background": "var(--wp--preset--color--primary-500)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--lg)"
        },
        "spacing": {
          "padding": {
            "top": "0.7em",
            "bottom": "0.7em",
            "left": "1.2em",
            "right": "1.2em"
          }
        }
      },
      "core/post-navigation-link": {
        "spacing": {
          "margin": {
            "top": "var(--wp--custom--spacing--small)",
            "bottom": "var(--wp--custom--spacing--small)"
          }
        }
      },
      "core/query-pagination": {
        "spacing": {
          "margin": {
            "top": "var(--wp--custom--spacing--large)",
            "bottom": "var(--wp--custom--spacing--large)"
          }
        }
      },
      "core/term-description": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--headings)",
          "fontSize": "20px"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}
